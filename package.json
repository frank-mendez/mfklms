{
  "name": "mfklms",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "prisma generate && next build",
    "build:dev": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "postinstall": "prisma generate",
    "prepare": "husky",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:seed": "prisma db seed",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset --force",
    "db:setup": "prisma generate && prisma db push && npm run db:seed",
    "db:deploy": "prisma migrate deploy",
    "build:production": "npm run db:generate && npm run db:deploy && npm run db:seed && npm run build",
    "build:vercel": "prisma generate && prisma migrate deploy && prisma db seed && next build",
    "deploy": "npm run build:production && npm run start"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.9.1",
    "@prisma/client": "^6.9.0",
    "@tanstack/react-query": "^5.85.5",
    "@tanstack/react-query-devtools": "^5.85.5",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "cally": "^0.8.0",
    "daisyui": "^5.0.50",
    "husky": "^9.1.7",
    "jest": "^30.1.2",
    "next": "15.3.2",
    "next-auth": "^4.24.11",
    "react": "^19.0.0",
    "react-calendar": "^6.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4.1.12",
    "@tanstack/eslint-plugin-query": "^5.83.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20.19.0",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "eslint": "^9",
    "eslint-config-next": "15.3.2",
    "postcss": "^8.5.6",
    "prisma": "^6.9.0",
    "tailwindcss": "^4.1.12",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  }
}
